{
    "type": "library",
    "name": "inspirecz/coding-standard",
    "description": "INSPIRE Coding Standard",
    "license": "MIT",
    "keywords": [
        "coding standard",
        "php",
        "coding",
        "standard"
    ],
    "authors": [
        {
            "name": "INSPIRE CZ s.r.o.",
            "email": "support@inspire.cz",
            "homepage": "https://www.inspire.cz/"
        }
    ],
    "require": {
        "php": ">=8.3",
        "squizlabs/php_codesniffer": "^4",
        "inspirecz/coding-standard-sniffs": "2.0.x-dev",
        "slevomat/coding-standard": "^8"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "platform": {
            "php": "8.5.0"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        }
    }
}
